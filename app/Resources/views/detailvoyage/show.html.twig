{% extends 'base.html.twig' %}

{% block body %}
    <h1>Detailvoyage</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ detailVoyage.id }}</td>
            </tr>
            <tr>
                <th>Dateajout</th>
                <td>{% if detailVoyage.dateAjout %}{{ detailVoyage.dateAjout|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('detailvoyage_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('detailvoyage_edit', { 'id': detailVoyage.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
